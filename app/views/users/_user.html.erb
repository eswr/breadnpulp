<tr>
	<td>
		<%= user.id %>
	</td>
	<td>
		<%= user.name %>
	</td>
	<td>
		<%= user.phone_number %>
	</td>
	<td>
		<%= user.email %>
	</td>
	<td>
		<%= user.source %>
	</td>
	<td>
		<%= user.created_at + 5.hours + 30.minutes %>
	</td>
	<td>
		<%= user.admin? %>
	</td>
	<td>
		<%= link_to 'edit', edit_user_path user %>
	</td>
</tr>