<div class="col-md-6 col-md-offest-1">	
	<div id="kickerr">
		<li>
			<div class="row">
				<div class="center">
					<h3><%= kickerr.name %></h3>
				</div>
			  	<% courses_array = ['Main', 'Beverage', 'Salad', 'Side'] %>
				<div class="row">
					<% courses_array.each do |c| %>
						<div class="col-md-4">
							<% kickerr.food_items.where(course: c).each do |food_item| %>
								<%= food_item.name %><br>
								<%= image_tag food_item.image.url :thumb %>
							<% end %>
						</div>
					<% end %>
				</div>
			</div>
		</li>
	</div>
</div>
